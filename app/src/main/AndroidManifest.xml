        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_black"
        android:supportsRtl="true"
        android:theme="@style/Theme.Practic"
        tools:targetApi="31">
        <activity
            android:name="com.example.team.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.team.AboutActivity"
            android:exported="true" />
        <activity
            android:name="com.example.team.CountryHousesActivity"
            android:exported="true" />
        <activity
            android:name="com.example.team.FeedbackActivity"
            android:exported="true" />
        <activity
            android:name="com.example.team.MediafileActivity"
            android:exported="true" />
        <activity
            android:name="com.example.team.ProjectActivity"
            android:exported="true" />
        <activity
            android:name="com.example.team.PublicationActivity"
            android:exported="true" />
        <activity
            android:name="com.example.team.RewardActivity"
            android:exported="true" />
        <activity
            android:name="com.example.team.ServicesActivity"
            android:exported="true" />
        <activity
            android:name="com.example.team.ActivityMenu"
            android:exported="true" />
    </application>

</manifest>